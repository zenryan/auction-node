{
  "name": "auction-node",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./dist/bin/www",
    "build": "yarn tsc",
    "prebuild": "del './dist'",
    "postbuild": "cpy '**/*' '!**/*.ts' './../dist' --cwd=src --parents",
    "dev": "nodemon --ext js,ts,json --watch src --exec 'ts-node' ./src/bin/www",
    "tsc": "tsc"
  },
  "dependencies": {
    "@types/express": "^4.17.7",
    "@types/node": "^14.0.27",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "express-routemagic": "^2.0.2",
    "hbs": "~4.0.4",
    "http-errors": "~1.6.3",
    "morgan": "~1.9.1",
    "typescript": "^3.9.7"
  },
  "devDependencies": {
    "cpy-cli": "^3.1.1",
    "del-cli": "^3.0.1",
    "nodemon": "^2.0.4",
    "ts-node": "^8.10.2"
  }
}
