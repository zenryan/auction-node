{
  "name": "auction-node",
  "version": "0.1.2",
  "private": true,
  "scripts": {
    "build": "yarn tsc",
    "prebuild": "del './dist'",
    "postbuild": "cpy '**/*' '!**/*.ts' './../dist' --cwd=src --parents",
    "start": "NODE_ENV=production node ./dist/bin/www",
    "dev": "NODE_ENV=development nodemon --ext js,ts,json --watch src --exec 'ts-node' ./src/bin/www",
    "tsc": "tsc",
    "typeorm": "node --require ts-node/register ./node_modules/typeorm/cli.js"
  },
  "dependencies": {
    "@types/cors": "^2.8.7",
    "@types/express": "^4.17.7",
    "@types/node": "^14.0.27",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "dotenv": "^8.2.0",
    "express": "~4.16.1",
    "express-routemagic": "^2.0.2",
    "hbs": "~4.0.4",
    "http-errors": "~1.6.3",
    "morgan": "~1.9.1",
    "mysql": "^2.18.1",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.2.25",
    "typescript": "^3.9.7",
    "you-are-not": "^0.7.5"
  },
  "devDependencies": {
    "cpy-cli": "^3.1.1",
    "del-cli": "^3.0.1",
    "nodemon": "^2.0.4",
    "ts-node": "^8.10.2"
  },
  "license": "Private"
}
